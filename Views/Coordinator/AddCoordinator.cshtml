@model Coordinator
<div class="text-center">
    <h1 class="display-4" style="font-size: 40px">Agregar coordinador</h1>
    <form asp-action="AddCoordinator" controller="Coordinator" method="post">

        <div class="form-group">
            <label for="Name">Nombre:</label>
            <input type="text" asp-for="Name" class="form-control" id="name" name="name">
        </div>
        <div class="form-group">
            <label for="PhoneNumber">Teléfono:</label>
            <input type="text" asp-for="PhoneNumber" class="form-control" id="phoneNumber" name="phoneNumber">
        </div>

        <button type="submit" class="btn btn-primary">Agregar coordinador</button>
    </form>

</div>